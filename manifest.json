{

    "manifest_version": 2,

    "name": "UX Mockup",

    "version": "0.1",

    "description": "UX Mockup",

    "icons": {
        "48": "icons/princeton_university_shield_book_gray.svg",
        "96": "icons/princeton_university_shield_book_gray.svg"
    },

    "background": {
        "scripts": [ "unprivileged.js" ]
    },

    "permissions": [
        "notifications"
    ],

    "browser_specific_settings": {
        "gecko": {
            "id": "news.study.ux.mockup@princeton.edu",
            "strict_min_version": "60.0"
        }
    },

    "experiment_apis": {
        "privileged": {
            "schema": "./privileged.json",
            "parent": {
                "scopes": ["addon_parent"],
                "script": "./privileged.js",
                "paths": [["privileged"]]
            }
        }
    }
}
